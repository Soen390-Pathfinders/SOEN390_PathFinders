{
  "name": "frontend",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --coverage",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}",
      "!app/**/*.d.ts",
      "!app/**/styles.{js,ts,tsx}",
      "!app/**/constants.{js,ts,tsx}",
      "!app/**/index.{js,ts,tsx}",
      "!app/data/**",
      "!app/**/types.{js,ts,tsx}",
      "!app/assets/**",
      "!app/(tabs)/**",
      "!app/components/maps/building_outlines.tsx",
      "!app/components/maps/concordiaBuildings.ts",
      "!**/node_modules/**",
      "!**/tests/**"
    ]
